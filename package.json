{
  "name": "feedback-analyzer",
  "version": "1.0.0",
  "description": "Cloudflare-powered feedback aggregation and analysis tool",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create feedback-db",
    "db:migrate:local": "wrangler d1 execute feedback-db --local --file=src/db/schema.sql",
    "db:migrate:remote": "wrangler d1 execute feedback-db --file=src/db/schema.sql",
    "db:seed:local": "wrangler d1 execute feedback-db --local --file=src/db/seed.sql",
    "db:seed:remote": "wrangler d1 execute feedback-db --file=src/db/seed.sql"
  },
  "keywords": ["cloudflare", "workers", "d1", "ai", "feedback"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.91.0"
  }
}
